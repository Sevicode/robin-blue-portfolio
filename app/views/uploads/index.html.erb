<p id="notice"><%= notice %></p>

<h1>Listing Art Works</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uploads.each do |upload| %>
      <tr>
        <td><%= link_to 'Show', upload, class: "btn btn-link" %></td>
        <td><%= link_to 'Edit', edit_upload_path(upload), class: "btn btn-link" %></td>
        <td><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-link" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload', new_upload_path, class: "btn btn-link" %>
